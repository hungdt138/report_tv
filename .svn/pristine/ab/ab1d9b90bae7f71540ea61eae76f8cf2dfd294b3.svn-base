package com.tv.xeeng.reporttool.forms;

import org.apache.struts.action.ActionForm;

public class GiftCodeForm extends ActionForm {
	private String giftCodeId;
	private String bonusMoney;
	private String serial;
	private String createdDate;
	private String usedDate;
	private int marketingChannelId;
	
	
	
	public GiftCodeForm(String giftCodeId, String bonusMoney, String serial,
			String createdDate, String usedDate, int marketingChannelId) {
		super();
		this.giftCodeId = giftCodeId;
		this.bonusMoney = bonusMoney;
		this.serial = serial;
		this.createdDate = createdDate;
		this.usedDate = usedDate;
		this.marketingChannelId = marketingChannelId;
	}

	public GiftCodeForm() {
		super();
		// TODO Auto-generated constructor stub
	}

	public String getGiftCodeId() {
		return giftCodeId;
	}

	public void setGiftCodeId(String giftCodeId) {
		this.giftCodeId = giftCodeId;
	}

	public String getBonusMoney() {
		return bonusMoney;
	}

	public void setBonusMoney(String bonusMoney) {
		this.bonusMoney = bonusMoney;
	}

	public String getSerial() {
		return serial;
	}

	public void setSerial(String serial) {
		this.serial = serial;
	}

	public String getCreatedDate() {
		return createdDate;
	}

	public void setCreatedDate(String createdDate) {
		this.createdDate = createdDate;
	}

	public String getUsedDate() {
		return usedDate;
	}

	public void setUsedDate(String usedDate) {
		this.usedDate = usedDate;
	}

	public int getMarketingChannelId() {
		return marketingChannelId;
	}

	public void setMarketingChannelId(int marketingChannelId) {
		this.marketingChannelId = marketingChannelId;
	}
	
	
	
}
