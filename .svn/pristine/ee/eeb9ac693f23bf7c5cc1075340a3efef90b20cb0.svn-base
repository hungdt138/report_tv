package com.tv.xeeng.reporttool.beans;

import javax.persistence.Column;
import javax.persistence.Entity;
import java.util.Date;

/**
 * Created by thanhnvt on 27/06/2014.
 */
@Entity
public class UserMoneyChange {
    @Column(name = "moneyDiff")
    private
    int moneyDiff;
    @Column(name = "moneyAfter")
    private
    int moneyAfter;
    @Column(name = "message")
    private
    String message;
    @Column(name = "modifyDate")
    private
    Date modifyDate;
    @Column(name = "reasonName")
    private
    String reason;

    public int getMoneyDiff() {
        return moneyDiff;
    }

    public void setMoneyDiff(int moneyDiff) {
        this.moneyDiff = moneyDiff;
    }

    public int getMoneyAfter() {
        return moneyAfter;
    }

    public void setMoneyAfter(int moneyAfter) {
        this.moneyAfter = moneyAfter;
    }

    public String getMessage() {
        return message;
    }

    public void setMessage(String message) {
        this.message = message;
    }

    public String getReason() {
        return reason;
    }

    public void setReason(String reason) {
        this.reason = reason;
    }

    public Date getModifyDate() {
        return modifyDate;
    }

    public void setModifyDate(Date modifyDate) {
        this.modifyDate = modifyDate;
    }
}
