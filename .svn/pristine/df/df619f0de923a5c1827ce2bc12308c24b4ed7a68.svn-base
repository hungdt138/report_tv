package com.tv.xeeng.reporttool.beans;

import javax.persistence.Column;
import javax.persistence.Entity;
import java.util.Date;

/**
 * Created by thanhnvt on 21/06/2014.
 */
@Entity
public class NumOfPayerByDay {
    @Column(name = "reportDate")
    private
    Date reportDate;
    @Column(name = "numOfPayerDaily")
    private
    int numOfPayer;
    @Column(name = "numOfNewPayerDaily")
    private
    int numOfNewPayer;
    @Column(name = "revenueOfNewPayerBySMSDaily")
    private
    int revenueOfNewPayerBySMSDaily;
    @Column(name = "revenueOfNewPayerByCardDaily")
    private
    int revenueOfNewPayerByCardDaily;

    public Date getReportDate() {
        return reportDate;
    }

    public void setReportDate(Date reportDate) {
        this.reportDate = reportDate;
    }

    public int getNumOfPayer() {
        return numOfPayer;
    }

    public void setNumOfPayer(int numOfPayer) {
        this.numOfPayer = numOfPayer;
    }

    public int getNumOfNewPayer() {
        return numOfNewPayer;
    }

    public void setNumOfNewPayer(int numOfNewPayer) {
        this.numOfNewPayer = numOfNewPayer;
    }

    public int getRevenueOfNewPayerBySMSDaily() {
        return revenueOfNewPayerBySMSDaily;
    }

    public void setRevenueOfNewPayerBySMSDaily(int revenueOfNewPayerBySMSDaily) {
        this.revenueOfNewPayerBySMSDaily = revenueOfNewPayerBySMSDaily;
    }

    public int getRevenueOfNewPayerByCardDaily() {
        return revenueOfNewPayerByCardDaily;
    }

    public void setRevenueOfNewPayerByCardDaily(int revenueOfNewPayerByCardDaily) {
        this.revenueOfNewPayerByCardDaily = revenueOfNewPayerByCardDaily;
    }
}

