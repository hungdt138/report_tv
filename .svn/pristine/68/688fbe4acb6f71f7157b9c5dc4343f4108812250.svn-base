package com.tv.xeeng.reporttool.forms;

import java.util.ArrayList;
import java.util.List;

import org.apache.struts.action.ActionForm;

import com.tv.xeeng.reporttool.beans.GoldenHourBean;

public class GoldenHourListForm extends ActionForm {

    private List<GoldenHourBean> gdhList;

    public GoldenHourListForm() {
        this.gdhList = new ArrayList<GoldenHourBean>();
    }

    public GoldenHourListForm(List<GoldenHourBean> gdhList) {
        super();
        this.gdhList = gdhList;
    }

    public List<GoldenHourBean> getGdhList() {
        return gdhList;
    }

    public void setGdhList(List<GoldenHourBean> gdhList) {
        this.gdhList = gdhList;
    }

}
